package variants_ptr

struct Simple {
  string name;
}

variant Expr {
  bool;
  int32;
}

// container type not supported yet
variant V {
  string;
  int32;
  double;
  Simple;
  Simple*;
  Simple[];
  int32[];
  Simple{int32};
  bytes[];
  string[];
  bytes{int32};
  bytes{string};
  Simple*[];
  Expr;
}

struct Value {
  V v;
}

struct ValueContainer {
  V[] vv;
  V{int32} vm;
}
